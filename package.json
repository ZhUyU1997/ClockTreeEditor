{
	"name": "clock-tree-editor",
	"displayName": "Clock Tree Editor for XBOOT",
	"description": "",
	"version": "0.0.2",
	"publisher": "yzhu",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"icon": "image/xboot-logo.png",
	"activationEvents": [
		"onCustomEditor:clock-tree-editor.xboot"
	],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "clock-tree-editor.xboot",
				"displayName": "open clock tree editor",
				"selector": [
					{
						"filenamePattern": "*.json"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"bugs": {
		"url": "https://github.com/ZhUyU1997/ClockTreeEditor/issues",
		"email": "891085309@qq.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ZhUyU1997/ClockTreeEditor.git"
	}
}